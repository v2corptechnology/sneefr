{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "algolia/algoliasearch-laravel": "^1.0",
        "aloha/twilio": "^2.1",
        "doctrine/dbal": "^2.5",
        "facebook/php-sdk-v4": "^5.0",
        "giggsey/libphonenumber-for-php": "^7.4",
        "guilhermegonzaga/presenter": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "hashids/hashids": "^1.0",
        "intervention/image": "^2.3",
        "jenssegers/rollbar": "^1.5",
        "laravel/cashier": "~6.0",
        "laravel/framework": "5.3.*",
        "laravelcollective/html": "^5.2",
        "league/color-extractor": "^0.3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nicolaslopezj/searchable": "^1.7",
        "pda/pheanstalk": "^3.0",
        "php": ">=5.6.4",
        "propaganistas/laravel-phone": "~2.0",
        "rap2hpoutre/laravel-log-viewer": "^0.7",
        "spatie/laravel-activitylog": "^1.2",
        "vinkla/algolia": "^2.2",
        "vinkla/pusher": "^2.2",
        "willdurand/negotiation": "^2.0",
        "spatie/laravel-failed-job-monitor": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.2",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Sneefr\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
